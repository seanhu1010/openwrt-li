
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option device 'eth0'
	option proto 'static'
	option ipaddr '10.1.1.2'
	option netmask '255.255.255.0'
	option gateway '10.1.1.1'
	list dns '218.201.17.2'
	option delegate '0'

config globals 'globals'
	option packet_steering '1'
	option ula_prefix 'fdac::/64'

config interface 'lan6'
	option proto 'dhcpv6'
	option device 'eth0'
	option reqaddress 'try'
	option reqprefix 'auto'
	option peerdns '0'
	list dns '2409:8060:20ea:100::1'
	option ip6ifaceid '::2'
	option ip6assign '64'

config interface 'utun'
	option proto 'none'
	option ifname 'utun'
	option device 'utun'
	option auto '0'

config interface 'wg1'
	option proto 'wireguard'
	option private_key '8NpQ7f+CTYYqolG9FBTWe+QwrKMbBvyD8uWtOpKm3Hk='
	option listen_port '11111'
	list addresses '192.168.11.1/24'
	option nohostroute '1'

config wireguard_wg1
	option public_key 'tjjTywvjar3jCgx3q7qJdErEAwoGPKTmcKFIHdkkq2I='
	option private_key 'YInz6DYT/Jrd5bjdr0qj4pU9ng2IF24bHEOAoBxw3W8='
	option preshared_key '0N0ZQ4U+co9wsly+y5+nmnM/p++IlJYwdR5YAe5Qrh8='
	option endpoint_port '11111'
	option persistent_keepalive '25'
	list allowed_ips '192.168.11.2/32'
	option description 'client'
	option route_allowed_ips '1'

